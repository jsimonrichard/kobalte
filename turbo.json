{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["@trellis-app/kobalte-tests#build", "@trellis-app/kobalte-utils#build"]
		},
		"typecheck": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
